package com.website.imgservice.entities;

public class Image {
	private int id;
	private String title;
	private String url;
	
	public Image(int id, String title, String url){
		this.setId(id);
		this.title=title;
		this.setUrl(url);
	}

	public int getId() {
		return id;
	}

	public void setId(int id) {
		this.id = id;
	}

	public String getUrl() {
		return url;
	}

	public void setUrl(String url) {
		this.url = url;
	}
}
